#include <stdio.h>

int main()
{
    int sum=0,n;
                for(int n=0;n<1000;n++)
                {
                   if(n%3==0||n%5==0)
                   {
                   sum+=n;
                   }
                }
            printf("sum of all multiples of 3 or 5 below 1000 is: %d",sum);
    return 0;
}
